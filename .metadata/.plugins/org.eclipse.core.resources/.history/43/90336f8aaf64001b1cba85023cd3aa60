import java.util.Scanner;
public class Neevennumbersusingwhile{
	 
    public static void main(String[] args) {
        //number
        int n = 20;
         
        //print all even numbers <=n 
        int i=1;
        while (i<=n) {
            //check if i is exactly divisible by 2
            if (i%2==0) {
                System.out.print(i+"  ");
            }
            i++;
        }
    }
}